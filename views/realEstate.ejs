    <script>
        document.addEventListener('DOMContentLoaded', function () {
            document.querySelectorAll('.stellar-image-slider').forEach(function (slider) {
                const slides = slider.querySelectorAll('.nexus-slide-item');
                const dots = slider.parentElement.querySelectorAll('.cosmic-dot');
                let current = 0;
                let startX = 0;
                let isDragging = false;
                let isMouse = false;

                function showSlide(idx) {
                    if (idx < 0) idx = 0;
                    if (idx >= slides.length) idx = slides.length - 1;
                    slider.style.transform = `translateX(-${idx * 100}%)`;
                    dots.forEach((dot, i) => {
                        dot.classList.toggle('active', i === idx);
                    });
                    current = idx;
                }

                // Prevent default drag on images
                slides.forEach(slide => {
                    const img = slide.querySelector('img');
                    if (img) {
                        img.ondragstart = e => e.preventDefault();
                        img.style.cursor = 'grab';
                    }
                });

                // Initialize
                showSlide(0);

                // Dot navigation
                dots.forEach((dot, i) => {
                    dot.addEventListener('click', function () {
                        showSlide(i);
                    });
                });

                // Touch/swipe navigation
                slider.addEventListener('touchstart', function (e) {
                    if (e.touches.length === 1) {
                        startX = e.touches[0].clientX;
                        isDragging = true;
                    }
                });

                slider.addEventListener('touchmove', function (e) {
                    if (isDragging) e.preventDefault();
                }, { passive: false });

                slider.addEventListener('touchend', function (e) {
                    if (!isDragging) return;
                    isDragging = false;
                    const endX = e.changedTouches[0].clientX;
                    const diff = endX - startX;
                    if (Math.abs(diff) > 40) {
                        if (diff < 0 && current < slides.length - 1) {
                            showSlide(current + 1);
                        } else if (diff > 0 && current > 0) {
                            showSlide(current - 1);
                        }
                    }
                });

                // Mouse drag navigation
                slider.addEventListener('mousedown', function (e) {
                    isMouse = true;
                    isDragging = true;
                    startX = e.clientX;
                    slider.style.cursor = 'grabbing';
                });

                document.addEventListener('mousemove', function (e) {
                    if (isDragging && isMouse) {
                        // Optionally, you can add visual feedback here
                    }
                });

                document.addEventListener('mouseup', function (e) {
                    if (!isDragging || !isMouse) return;
                    isDragging = false;
                    isMouse = false;
                    slider.style.cursor = 'grab';
                    const endX = e.clientX;
                    const diff = endX - startX;
                    if (Math.abs(diff) > 40) {
                        if (diff < 0 && current < slides.length - 1) {
                            showSlide(current + 1);
                        } else if (diff > 0 && current > 0) {
                            showSlide(current - 1);
                        }
                    }
                });
            });
        });

    </script>
