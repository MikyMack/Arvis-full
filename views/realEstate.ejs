    <script>
        let lastScroll = 0;
        const whatsappIcon = document.getElementById('whatsappIcon');

        window.addEventListener('scroll', () => {
            const currentScroll = window.pageYOffset;


            if (currentScroll <= 0) {
                whatsappIcon.classList.remove('show');
                return;
            }


            if (currentScroll > lastScroll) {
                whatsappIcon.classList.add('show');
            }

            else {
                whatsappIcon.classList.remove('show');
            }

            lastScroll = currentScroll;
        });

        // hero slider

        // Get the dynamic number of slides from realestateBanner
        const arviesSlider = {
            currentSlideIndex: 0,
            totalSlides: Array.isArray(realestateBanner) ? realestateBanner.length : 0,
            slideInterval: null,
            autoSlideDelay: 5000, // 5 seconds
            progressInterval: null,

            init() {
                // If there are no slides, do not initialize
                if (this.totalSlides === 0) return;
                this.setupEventListeners();
                this.startAutoSlide();
                this.updateNavDots();
            },

            setupEventListeners() {
                // Touch/Swipe functionality
                let startX = 0;
                let endX = 0;
                const slidesContainer = document.getElementById('arviesSlides');
                if (!slidesContainer) return;

                slidesContainer.addEventListener('touchstart', (e) => {
                    startX = e.touches[0].clientX;
                });

                slidesContainer.addEventListener('touchend', (e) => {
                    endX = e.changedTouches[0].clientX;
                    this.handleSwipe(startX, endX);
                });

                // Mouse drag functionality
                let isDragging = false;

                slidesContainer.addEventListener('mousedown', (e) => {
                    isDragging = true;
                    startX = e.clientX;
                });

                slidesContainer.addEventListener('mousemove', (e) => {
                    if (!isDragging) return;
                    e.preventDefault();
                });

                slidesContainer.addEventListener('mouseup', (e) => {
                    if (!isDragging) return;
                    isDragging = false;
                    endX = e.clientX;
                    this.handleSwipe(startX, endX);
                });

                // Pause auto-slide on hover
                const sliderWrapper = document.querySelector('.arvies-slider-wrapper');
                if (sliderWrapper) {
                    sliderWrapper.addEventListener('mouseenter', () => this.pauseAutoSlide());
                    sliderWrapper.addEventListener('mouseleave', () => this.startAutoSlide());
                }
            },

            handleSwipe(startX, endX) {
                const threshold = 50;
                const diff = startX - endX;

                if (Math.abs(diff) > threshold) {
                    if (diff > 0) {
                        this.nextSlide();
                    } else {
                        this.prevSlide();
                    }
                }
            },

            updateSlidePosition() {
                const slidesContainer = document.getElementById('arviesSlides');
                if (!slidesContainer) return;
                if (this.totalSlides === 0) return;
                const translateX = -(this.currentSlideIndex * (100 / this.totalSlides));
                slidesContainer.style.transform = `translateX(${translateX}%)`;
                this.updateNavDots();
            },

            updateNavDots() {
                const dots = document.querySelectorAll('.arvies-nav-dot');
                dots.forEach((dot, index) => {
                    if (index === this.currentSlideIndex) {
                        dot.classList.add('arvies-active');
                    } else {
                        dot.classList.remove('arvies-active');
                    }
                });
            },

            nextSlide() {
                if (this.totalSlides === 0) return;
                this.currentSlideIndex = (this.currentSlideIndex + 1) % this.totalSlides;
                this.updateSlidePosition();
                this.resetAutoSlide();
            },

            prevSlide() {
                if (this.totalSlides === 0) return;
                this.currentSlideIndex = (this.currentSlideIndex - 1 + this.totalSlides) % this.totalSlides;
                this.updateSlidePosition();
                this.resetAutoSlide();
            },

            currentSlide(slideIndex) {
                if (this.totalSlides === 0) return;
                this.currentSlideIndex = Math.max(0, Math.min(slideIndex - 1, this.totalSlides - 1));
                this.updateSlidePosition();
                this.resetAutoSlide();
            },

            startAutoSlide() {
                if (this.totalSlides <= 1) return; // Don't auto-slide if only one or zero slides
                this.pauseAutoSlide();
                this.startProgressBar();
                this.slideInterval = setInterval(() => {
                    this.nextSlide();
                }, this.autoSlideDelay);
            },

            pauseAutoSlide() {
                if (this.slideInterval) {
                    clearInterval(this.slideInterval);
                    this.slideInterval = null;
                }
                this.pauseProgressBar();
            },

            resetAutoSlide() {
                this.startAutoSlide();
            },

            startProgressBar() {
                const progressBar = document.getElementById('arviesProgressBar');
                if (!progressBar) return;
                let progress = 0;
                const increment = 100 / (this.autoSlideDelay / 100);

                this.pauseProgressBar();
                this.progressInterval = setInterval(() => {
                    progress += increment;
                    if (progress >= 100) {
                        progress = 0;
                    }
                    progressBar.style.width = progress + '%';
                }, 100);
            },

            pauseProgressBar() {
                if (this.progressInterval) {
                    clearInterval(this.progressInterval);
                    this.progressInterval = null;
                }
                const progressBar = document.getElementById('arviesProgressBar');
                if (progressBar) {
                    progressBar.style.width = '0%';
                }
            }
        };

        // Initialize slider when DOM is loaded
        document.addEventListener('DOMContentLoaded', () => {
            arviesSlider.init();
        });

        // Keyboard navigation
        document.addEventListener('keydown', (e) => {
            if (arviesSlider.totalSlides === 0) return;
            if (e.key === 'ArrowLeft') {
                arviesSlider.prevSlide();
            } else if (e.key === 'ArrowRight') {
                arviesSlider.nextSlide();
            }
        });
    </script>

