                <div class="galaxy-properties-grid" id="properties-list">
                    <% realestate.forEach(function(property, idx) { %>
                        <div class="prismo-property-showcase" data-category="<%= property.category %>">
                            <div class="luxe-image-container">
                                <div class="stellar-image-slider" data-slider="<%= idx %>">
                                    <% if (property.images && property.images.length) { %>
                                        <% property.images.forEach(function(img, imgIdx) { %>
                                            <div class="nexus-slide-item">
                                                <img src="<%= img.url %>" alt="<%= img.seoAlt && img.seoAlt.trim() ? img.seoAlt : (property.title + ' Image ' + (imgIdx+1)) %>">
                                            </div>
                                        <% }); %>
                                    <% } else { %>
                                        <div class="nexus-slide-item">
                                            <img src="/img/placeholder-property.jpg" alt="No Image">
                                        </div>
                                    <% } %>
                                </div>
                                <div class="quantum-sale-badge">
                                    <%= property.category && property.category.toLowerCase() === 'rent' ? 'For Rent' : 'For Sale' %>
                                </div>
                                <div class="aurora-navigation-dots">
                                    <% if (property.images && property.images.length) { %>
                                        <% property.images.forEach(function(img, imgIdx) { %>
                                            <span class="cosmic-dot <%= imgIdx === 0 ? 'active' : '' %>" data-slide="<%= imgIdx %>"></span>
                                        <% }); %>
                                    <% } else { %>
                                        <span class="cosmic-dot active" data-slide="0"></span>
                                    <% } %>
                                </div>
                            </div>
                            <div class="zenith-property-details">
                                <% if (property.title) { %>
                                    <h2 class="apex-property-title"><%= property.title %></h2>
                                <% } %>
                                <% if (property.location) { %>
                                    <div class="eclipse-location-wrapper">
                                        <svg class="eclipse-location-icon" viewBox="0 0 24 24">
                                            <path d="M12 2C8.13 2 5 5.13 5 9c0 5.25 7 13 7 13s7-7.75 7-13c0-3.87-3.13-7-7-7zm0 9.5c-1.38 0-2.5-1.12-2.5-2.5s1.12-2.5 2.5-2.5 2.5 1.12 2.5 2.5-1.12 2.5-2.5 2.5z" />
                                        </svg>
                                        <span class="vortex-location-text"><%= property.location %></span>
                                    </div>
                                <% } %>
                                <% if (property.area || property.price) { %>
                                    <div class="d-flex align-items-center justify-content-between" style="gap: 12px; margin-bottom: 10px;">
                                        <% if (property.area) { %>
                                            <div class="titan-spec-item" style="margin-bottom: 0;">
                                                <svg class="titan-spec-icon" viewBox="0 0 24 24">
                                                    <path d="M5 19h14V5H5v14zm2-12h10v2H7V7zm0 4h10v2H7v-2zm0 4h10v2H7v-2z" />
                                                </svg>
                                                <span class="nebula-spec-text"><%= property.area %> sqft</span>
                                            </div>
                                        <% } %>
                                        <% if (property.price) { %>
                                            <div class="infinity-price-section" style="margin-bottom: 0;">
                                                <div class="stellar-price-amount">
                                                    <span class="currency-symbol">₹</span>
                                                    <%= property.price %>
                                                </div>
                                            </div>
                                        <% } %>
                                    </div>
                                <% } %>
                                <a href="/projectDetails/<%= property._id %>">
                                    <button class="prism-action-button">More Details</button>
                                </a>
                            </div>
                        </div>
                    <% }); %>
