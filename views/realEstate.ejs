                                    <% if (property.images && property.images.length) { %>
                                        <% property.images.forEach(function(img, imgIdx) { %>
                                            <span class="cosmic-dot <%= imgIdx === 0 ? 'active' : '' %>" data-slide="<%= imgIdx %>"></span>
