<script>
    // Base cost data from the PDFs
    const residentialBaseCosts = {
        "1": { "classic": 1200, "elite": 1800, "luxury": 2500 },
        "2": { "classic": 1300, "elite": 1950, "luxury": 2700 },
        "3": { "classic": 1400, "elite": 2100, "luxury": 2900 },
        "4": { "classic": 1500, "elite": 2200, "luxury": 3100 },
        "5": { "classic": 1600, "elite": 2400, "luxury": 3300 }
    };
    
    const commercialBaseCosts = {
        "office": { "classic": 1200, "elite": 1800, "luxury": 2500 },
        "retail": { "classic": 1500, "elite": 2200, "luxury": 3000 },
        "f&b": { "classic": 1600, "elite": 2400, "luxury": 3200 },
        "coworking": { "classic": 1300, "elite": 1900, "luxury": 2600 },
        "gym": { "classic": 1400, "elite": 2100, "luxury": 2800 },
        "hospitality": { "classic": 1800, "elite": 2600, "luxury": 3500 }
    };
    
    const wardrobeMaterialCosts = {
        "plywood": 1200,
        "mdf": 1800,
        "hdhmr": 2500
    };
    
    // Modal functions
    function openModal() {
        document.getElementById('contactModal').style.display = 'flex';
    }
    
    function closeModal() {
        document.getElementById('contactModal').style.display = 'none';
    }
    
    // Close modal if clicked outside
    window.onclick = function(event) {
        const modal = document.getElementById('contactModal');
        if (event.target === modal) {
            closeModal();
        }
    };
    
    // Calculate residential cost
    function calculateResidential() {
        const bhk = document.getElementById('home-size').value;
        const area = document.getElementById('res-area').value;
        const quality = document.getElementById('res-quality').value;
        
        if (!bhk || !area || !quality) {
            alert('Please fill all fields');
            return;
        }
        
        const baseCost = residentialBaseCosts[bhk][quality];
        const totalCost = baseCost * area;
        
        // Store the calculation data
        const calculationData = {
            type: 'residential',
            bhk,
            area,
            quality,
            baseCost,
            totalCost,
            timestamp: new Date().toISOString()
        };
        
        showContactForm(calculationData);
    }
    
    // Calculate commercial cost
    function calculateCommercial() {
        const spaceType = document.getElementById('commercial-type').value;
        const area = document.getElementById('com-area').value;
        const quality = document.getElementById('com-quality').value;
        
        if (!spaceType || !area || !quality) {
            alert('Please fill all fields');
            return;
        }
        
        const baseCost = commercialBaseCosts[spaceType][quality];
        const totalCost = baseCost * area;
        
        // Store the calculation data
        const calculationData = {
            type: 'commercial',
            spaceType,
            area,
            quality,
            baseCost,
            totalCost,
            timestamp: new Date().toISOString()
        };
        
        showContactForm(calculationData);
    }
    
    // Calculate wardrobe cost
    function calculateWardrobe() {
        const width = document.getElementById('wardrobe-width').value;
        const height = document.getElementById('wardrobe-height').value;
        const material = document.getElementById('wardrobe-material').value;
        
        if (!width || !height || !material) {
            alert('Please fill all fields');
            return;
        }
        
        const area = width * height;
        const materialCost = wardrobeMaterialCosts[material];
        const totalCost = area * materialCost;
        
        // Store the calculation data
        const calculationData = {
            type: 'wardrobe',
            width,
            height,
            material,
            area,
            materialCost,
            totalCost,
            timestamp: new Date().toISOString()
        };
        
        showContactForm(calculationData);
    }
    
    // Show contact form in modal
    function showContactForm(calculationData) {
        const modalContent = document.getElementById('modal-content');
        
        modalContent.innerHTML = `
            <h3 class="luxe-modal-title" style="color: #000;">Almost Done!</h3>
            <p style="color: #000;">Please provide your details to see the estimate</p>
            <form class="luxe-modal-form" onsubmit="saveSubmission(event, ${JSON.stringify(calculationData).replace(/"/g, '&quot;')})">
                <div class="luxe-input-group">
                    <label style="color: #000;">Full Name</label>
                    <input type="text" class="luxe-input" placeholder="Your Name" required>
                </div>
                <div class="luxe-input-group">
                    <label style="color: #000;">Email Address</label>
                    <input type="email" class="luxe-input" placeholder="your.email@example.com" required>
                </div>
                <div class="luxe-input-group">
                    <label style="color: #000;">Mobile Number</label>
                    <input type="tel" class="luxe-input" placeholder="+91 1234567890" required>
                </div>
                <button type="submit" class="luxe-modal-btn" style="color: #000;">Submit & See Estimate</button>
            </form>
        `;
        
        openModal();
    }
    
    // Save submission and show success message
    async function saveSubmission(event, calculationData) {
      event.preventDefault();
      
      const form = event.target;
      const name = form.querySelector('input[type="text"]').value;
      const email = form.querySelector('input[type="email"]').value;
      const phone = form.querySelector('input[type="tel"]').value;
      
      try {
        // Send data to backend
        const response = await fetch('/api/submissions', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            name,
            email,
            phone,
            calculationType: calculationData.type,
            calculationData: calculationData,
            totalCost: calculationData.totalCost
          })
        });
        
        const result = await response.json();
        
        if (result.success) {
          // Show success message
          const modalContent = document.getElementById('modal-content');
          modalContent.innerHTML = `
            <div class="luxe-success-message">
              <div class="luxe-success-icon">
                <i class="fas fa-check-circle"></i>
              </div>
              <h3 class="luxe-success-title" style="color: #000;">Thank You!</h3>
              <p class="luxe-success-text" style="color: #000;">Our team will contact you shortly with your personalized estimate.</p>
              <button class="luxe-modal-btn" style="color: #000;" onclick="closeModal()">Finish</button>
            </div>
          `;
        } else {
          alert('Error saving your submission. Please try again.');
        }
      } catch (error) {
        console.error('Error:', error);
        alert('Error saving your submission. Please try again.');
      }
    }
</script>

