   <% 
   var videoBanner = Array.isArray(interiorBanner)
       ? interiorBanner.find(function(b) { return b && b.link && b.link.includes('youtube.com'); })
       : null;

   function getYouTubeId(url) {
       if (!url) return null;
       var match = url.match(/(?:youtube\.com\/.*v=|youtu\.be\/)([a-zA-Z0-9_-]{11})/);
       return match ? match[1] : null;
   }

   var videoId = videoBanner ? getYouTubeId(videoBanner.link) : null;
%>
<div class="bs-video-1-area wa-fix">
   <div class="bs-video-1-content wa-p-relative">
       <div class="bs-video-1-content-img has-video-2 wa-p-relative wa-fix wa-img-cover">
           <% if (videoId) { %>
               <div class="wa-parallax-img" style="position:relative;padding-bottom:56.25%;height:0;overflow:hidden;">
                   <iframe 
                       src="https://www.youtube.com/embed/<%= videoId %>?autoplay=1&mute=1&loop=1&playlist=<%= videoId %>" 
                       frameborder="0" 
                       allow="autoplay; encrypted-media" 
                       allowfullscreen
                       style="position:absolute;top:0;left:0;width:100%;height:100%;">
                   </iframe>
               </div>
           
           <% } else { %>
               <div class="wa-parallax-img" style="background:#000;height:350px;display:flex;align-items:center;justify-content:center;">
                   <span style="color:#fff;">No video available</span>
               </div>
           <% } %>
       </div>
   </div>
</div>

<section class="luxe-interior-calculator">
    <div class="luxe-container">
        <div class="luxe-header">
            <h2 class="luxe-title">Interior Cost Calculator</h2>
            <p class="luxe-subtitle">Get instant estimates for your dream interiors</p>
        </div>

        <div class="luxe-calculator-grid">
            <!-- Residential Card -->
            <div class="luxe-calc-card full-home-card">
                <div class="luxe-card-badge">Most Popular</div>
                <div class="luxe-card-icon">
                    <i class="fa-solid fa-home"></i>
                </div>
                <h3 style="color: #000;">Residential Interior</h3>
                <p style="color: #000;">Know the estimate price for your full home interiors</p>
                <div class="luxe-calc-form">
                    <div class="luxe-input-group">
                        <label style="color: #000;">Home Size (BHK)</label>
                        <select class="luxe-select" id="home-size">
                            <option value="">Select BHK</option>
                            <option value="1">1 BHK</option>
                            <option value="2">2 BHK</option>
                            <option value="3">3 BHK</option>
                            <option value="4">4 BHK</option>
                            <option value="5">5 BHK+</option>
                        </select>
                    </div>
                    <div class="luxe-input-group">
                        <label style="color: #000;">Area (sq.ft)</label>
                        <input type="number" id="res-area" placeholder="E.g. 1200" class="luxe-input">
                    </div>
                    <div class="luxe-input-group">
                        <label style="color: #000;">Quality Level</label>
                        <select class="luxe-select" id="res-quality">
                            <option value="">Select Quality</option>
                            <option value="classic">Classic</option>
                            <option value="elite">Elite</option>
                            <option value="luxury">Luxury</option>
                        </select>
                    </div>
                    <button class="luxe-calc-btn" onclick="calculateResidential()">
                        Calculate Estimate <i class="fa-solid fa-calculator"></i>
                    </button>
                    <div class="luxe-result">
                        <div class="luxe-result-placeholder">
                            <i class="fa-solid fa-spinner fa-spin"></i> Submit to see estimate
                        </div>
                    </div>
                </div>
            </div>

