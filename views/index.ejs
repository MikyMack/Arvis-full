                    const moreProjectsText = document.getElementById('more-projects-text');
                    function updateMoreProjectsLink(filter) {
                        if (filter === 'architecture') {
                            moreProjectsLink.href = '/architectureDesigns';
                            moreProjectsText.textContent = 'More Architecture';
                        } else if (filter === 'interior') {
                            moreProjectsLink.href = '/interiorDesign';
                            moreProjectsText.textContent = 'More Interior';
                        } else if (filter === 'realestate') {
                            moreProjectsLink.href = '/realEstate';
