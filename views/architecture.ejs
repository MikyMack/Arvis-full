    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const slider = document.querySelector('.slider');
            const images = document.querySelectorAll('.slider img');
            const imageCount = images.length;
            let currentIndex = 0;

            function initializeSlider() {
                const firstImage = images[0].cloneNode(true);
                slider.appendChild(firstImage);
            }

            initializeSlider();

            function autoSlide() {
                currentIndex++;
                slider.style.transform = `translateX(-${currentIndex * 100}%)`;

                if (currentIndex >= imageCount) {
                    setTimeout(() => {
                        slider.style.transition = 'none';
                        currentIndex = 0;
                        slider.style.transform = 'translateX(0)';
                        void slider.offsetWidth; // Trigger reflow
                        slider.style.transition = 'transform 0.5s ease-in-out';
                    }, 500);
                }
            }

            let slideInterval = setInterval(autoSlide, 2000);

            // Optional hover pause
            slider.addEventListener('mouseenter', () => clearInterval(slideInterval));
            slider.addEventListener('mouseleave', () => slideInterval = setInterval(autoSlide, 2000));
        });
    </script>
