                <script>
                    // Initialize Swiper for category filter
                    document.addEventListener('DOMContentLoaded', function() {
                        if (window.Swiper) {
                            new Swiper('.category-filter-swiper', {
                                slidesPerView: 'auto',
                                spaceBetween: 10,
                                freeMode: true,
                                breakpoints: {
