                    });
                    const categories = Array.from(categoriesSet);
                %>
                <div class="d-flex justify-content-center flex-wrap gap-2 mb-5 " id="category-filters">
                    <a class="archi-btn" href="javascript:void(0);" data-category="All"><button>All</button></a>
                    <% categories.forEach(function(cat) { %>
                        <a class="archi-btn" href="javascript:void(0);" data-category="<%= cat %>"><button><%= cat %></button></a>
                    <% }) %>
                </div>

                <div class="project-grid pb-50" id="project-grid">
                    <% architectures.forEach(function(arch, idx) { %>
                        <div class="project-card" data-category="<%= arch.category %>">
                            <div class="image-slider-wrapper">
                                <a href="/architectureDetails/<%= arch._id %>">
                                    <div class="image-slider-container" data-slider="slider<%= idx %>">
                                        <% if (arch.images && arch.images.length > 0) { %>
                                            <% arch.images.forEach(function(img, imgIdx) { %>
                                                <img src="<%= img.url %>" alt="<%= img.seoAlt && img.seoAlt.length > 0 ? img.seoAlt : (arch.title || 'Architecture Image') %>" class="slider-image" style="<%= imgIdx === 0 ? '' : 'display:none;' %>">
                                            <% }) %>
                                        <% } else { %>
                                            <img src="/img/no-image.png" alt="No Image" class="slider-image">
                                        <% } %>
                                    </div>
                                </a>
                                <% if (arch.images && arch.images.length > 1) { %>
                                    <button class="slider-nav-buttons slider-prev" data-target="slider<%= idx %>">‹</button>
                                    <button class="slider-nav-buttons slider-next" data-target="slider<%= idx %>">›</button>
                                    <div class="slider-indicators" data-indicators="slider<%= idx %>">
                                        <% arch.images.forEach(function(img, imgIdx) { %>
                                            <div class="indicator-dot <%= imgIdx === 0 ? 'active-indicator' : '' %>" data-slide="<%= imgIdx %>"></div>
                                        <% }) %>
                                    </div>
                                <% } %>
                            </div>
                            <div class="project-details">
                                <a href="/architectureDetails/<%= arch._id %>">
                                    <h3 class="project-name"><%= arch.title %></h3>
                                </a>
                                <% if (arch.category) { %>
                                    <p class="project-category"><%= arch.category %></p>
                                <% } %>
                            </div>
                        </div>
                    <% }) %>
                </div>
            </div>

            <script>
                document.addEventListener('DOMContentLoaded', function() {
                    const filterLinks = document.querySelectorAll('#category-filters .archi-btn');
                    const cards = document.querySelectorAll('#project-grid .project-card');
                    filterLinks.forEach(link => {
                        link.addEventListener('click', function(e) {
                            e.preventDefault();
                            const cat = this.getAttribute('data-category');
                            filterLinks.forEach(l => l.classList.remove('active'));
                            this.classList.add('active');
                            cards.forEach(card => {
                                if (cat === 'All' || card.getAttribute('data-category') === cat) {
                                    card.style.display = '';
                                } else {
                                    card.style.display = 'none';
                                }
                            });
                        });
                    });

                    document.querySelectorAll('.image-slider-container').forEach(function(container) {
                        const images = container.querySelectorAll('.slider-image');
                        if (images.length <= 1) return;
                        let current = 0;
                        const sliderId = container.getAttribute('data-slider');
                        const prevBtn = document.querySelector('.slider-prev[data-target="' + sliderId + '"]');
                        const nextBtn = document.querySelector('.slider-next[data-target="' + sliderId + '"]');
                        const indicators = document.querySelectorAll('.slider-indicators[data-indicators="' + sliderId + '"] .indicator-dot');
                        function show(idx) {
                            images.forEach((img, i) => img.style.display = i === idx ? '' : 'none');
                            if (indicators.length) {
                                indicators.forEach((dot, i) => dot.classList.toggle('active-indicator', i === idx));
                            }
                        }
                        if (prevBtn && nextBtn) {
                            prevBtn.addEventListener('click', function() {
                                current = (current - 1 + images.length) % images.length;
                                show(current);
                            });
                            nextBtn.addEventListener('click', function() {
                                current = (current + 1) % images.length;
                                show(current);
                            });
                        }
                        if (indicators.length) {
                            indicators.forEach((dot, i) => {
                                dot.addEventListener('click', function() {
                                    current = i;
                                    show(current);
                                });
                            });
                        }
                        show(0);
                    });
                });
            </script>
