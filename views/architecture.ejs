                        border: 1px solid #f5e9c8;
                        background: #181818;
                        color: #f5e9c8;
                        padding: 7px 21px;
                        font-size: 1em;
                        transition: background 0.2s, color 0.2s;
                    }
                    .archi-btn.active button,
                    .archi-btn button:active,
                    .archi-btn button:focus {
                        background: #f5e9c8;
                        color: #181818;
                    }
                </style>

                    <div class="project-grid pb-50" id="project-grid">
                        <% architectures.forEach(function(arch, idx) { %>
                            <div class="project-card" data-category="<%= arch.category %>">
                                <div class="image-slider-wrapper" style="position:relative;">
                                    <a href="/architectureDetails/<%= arch._id %>">
                                        <div class="">
                                            <% if (arch.images && arch.images.length > 0) { 
                                                var img = arch.images[0]; %>
                                                <img src="<%= img.url %>"
                                                    alt="<%= img.seoAlt && img.seoAlt.length > 0 ? img.seoAlt : (arch.title || 'Architecture Image') %>"
                                                    class="slider-image">
                                            <% } else { %>
                                                <img src="/img/no-image.png" alt="No Image"
                                                    class="slider-image">
                                            <% } %>
                                            <% if (arch.category) { %>
                                                <span class="project-category-inside-image" style="position:absolute;top:12px;left:12px;z-index:2;color:white;padding:4px 12px;font-size:0.95em;pointer-events:none;">
                                                    <%= arch.category %>
                                                </span>
                                            <% } %>
                                        </div>
                                    </a>
                            
                                </div>
                                <div class="project-details">
                                    <a href="/architectureDetails/<%= arch._id %>">
                                        <h3 class="project-name">
                                            <%= arch.title %>
                                        </h3>
                                    </a>
                                    <!-- Category removed from details as per instruction -->
                                </div>
                            </div>
                        <% }) %>
