        <script>
            // Get current page URL
            const pageUrl = encodeURIComponent(window.location.href);
            const pageTitle = encodeURIComponent(document.title);

            // Facebook
            document.getElementById('share-facebook').href = `https://www.facebook.com/sharer/sharer.php?u=${pageUrl}`;
            document.getElementById('share-facebook').target = "_blank";

            // Instagram (Instagram does not support direct sharing via URL, so open Instagram homepage)
            document.getElementById('share-instagram').href = `https://www.instagram.com/`;
            document.getElementById('share-instagram').target = "_blank";

            // Twitter (now X)
            document.getElementById('share-twitter').href = `https://twitter.com/intent/tweet?url=${pageUrl}&text=${pageTitle}`;
            document.getElementById('share-twitter').target = "_blank";

            // Pinterest
            document.getElementById('share-pinterest').href = `https://pinterest.com/pin/create/button/?url=${pageUrl}&description=${pageTitle}`;
            document.getElementById('share-pinterest').target = "_blank";

            // Copy link
            document.getElementById('copy-link-btn').addEventListener('click', function(e) {
                e.preventDefault();
                navigator.clipboard.writeText(window.location.href).then(function() {
                    // Optionally show a tooltip or alert
                    alert('Link copied to clipboard!');
                });
            });
        </script>
